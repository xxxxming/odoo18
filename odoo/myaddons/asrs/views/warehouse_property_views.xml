<odoo>

    <record id="warehouse_property_view_list" model="ir.ui.view">
        <field name="name">warehouse_property_view_list</field>
        <field name="model">warehouse.property</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="state"/>
                <field name="inventory_action"/>
                <field name="source_target"/>
                <field name="new_target"/>
                <field name="frame_number"/>
                <field name="pack_barcode"/>
            </list>
        </field>
    </record>

    <record id="warehouse_property_act_window" model="ir.actions.act_window">
        <field name="name">自动仓库作业</field>
        <field name="res_model">warehouse.property</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>
