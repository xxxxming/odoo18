<odoo>

    <record id="control_system_view_list" model="ir.ui.view">
        <field name="name">control_system_view_list</field>
        <field name="model">control.system</field>
        <field name="arch" type="xml">
            <list>
<!--                <field name="life"/>-->
<!--                <field name="ready"/>-->
                <field name="factory"/>
                <field name="workshop"/>
                <field name="line"/>
                <field name="machine"/>
            </list>
        </field>
    </record>

    <record id="control_system_view_form" model="ir.ui.view">
        <field name="name">control_system_view_form</field>
        <field name="model">control.system</field>
        <field name="arch" type="xml">

            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="factory"/>
                        </group>

                    </group>
                    <notebook>
                        <page string="当前状态">
                            <group>
                            <field name="factory"/>
                            </group>
                        </page>
                        <page string="堆高机">
                            <group>
                            <field name="factory"/>
                            </group>
                        </page>
                        <page string="输入状态">
                            <group>
                                <group>
                                    <group>
                                        <field name="input_I00" string="输入 I0.0" readonly="True"/>
                                        <field name="input_I01" string="输入 I0.1"/>
                                        <field name="input_I02" string="输入 I0.2"/>
                                        <field name="input_I03" string="输入 I0.3"/>
                                        <field name="input_I04" string="输入 I0.4"/>
                                        <field name="input_I05" string="输入 I0.5"/>
                                        <field name="input_I06" string="输入 I0.6"/>
                                        <field name="input_I07" string="输入 I0.7"/>
                                    </group>
                                    <group>
                                        <field name="input_I10" string="输入 I1.0"/>
                                        <field name="input_I11" string="输入 I1.1"/>
                                        <field name="input_I12" string="输入 I1.2"/>
                                        <field name="input_I13" string="输入 I1.3"/>
                                        <field name="input_I14" string="输入 I1.4"/>
                                        <field name="input_I15" string="输入 I1.5"/>
                                        <field name="input_I16" string="输入 I1.6"/>
                                        <field name="input_I17" string="输入 I1.7"/>
                                    </group>
                                </group>
                                <group>
                                    <group>
                                        <field name="input_I00" string="输入 I2.0"/>
                                        <field name="input_I01" string="输入 I2.1"/>
                                        <field name="input_I02" string="输入 I2.2"/>
                                        <field name="input_I03" string="输入 I2.3"/>
                                        <field name="input_I04" string="输入 I2.4"/>
                                        <field name="input_I05" string="输入 I2.5"/>
                                        <field name="input_I06" string="输入 I2.6"/>
                                        <field name="input_I07" string="输入 I2.7"/>
                                    </group>
                                    <group>
                                        <field name="input_I00" string="输入 I3.0"/>
                                        <field name="input_I01" string="输入 I3.1"/>
                                        <field name="input_I02" string="输入 I3.2"/>
                                        <field name="input_I03" string="输入 I3.3"/>
                                        <field name="input_I04" string="输入 I3.4"/>
                                        <field name="input_I05" string="输入 I3.5"/>
                                        <field name="input_I06" string="输入 I3.6"/>
                                        <field name="input_I07" string="输入 I3.7"/>
                                    </group>
                                </group>
                            </group>
                        </page>
                        <page string="输出状态">
                            <group>
                                <group>
                                    <group>
                                        <field name="output_Q00" string="输出 Q0.0"/>
                                        <field name="output_Q00" string="输出 Q0.1"/>
                                        <field name="output_Q00" string="输出 Q0.2"/>
                                        <field name="output_Q00" string="输出 Q0.3"/>
                                        <field name="output_Q00" string="输出 Q0.4"/>
                                        <field name="output_Q00" string="输出 Q0.5"/>
                                        <field name="output_Q00" string="输出 Q0.6"/>
                                        <field name="output_Q00" string="输出 Q0.7"/>
                                    </group>
                                    <group>
                                        <field name="output_Q00" string="输出 Q.0"/>
                                        <field name="output_Q00" string="输出 Q1.1"/>
                                        <field name="output_Q00" string="输出 Q1.2"/>
                                        <field name="output_Q00" string="输出 Q1.3"/>
                                        <field name="output_Q00" string="输出 Q1.4"/>
                                        <field name="output_Q00" string="输出 Q1.5"/>
                                        <field name="output_Q00" string="输出 Q1.6"/>
                                        <field name="output_Q00" string="输出 Q1.7"/>
                                    </group>
                                </group>
                                <group>
                                    <group>
                                        <field name="output_Q00" string="输出 Q2.0"/>
                                        <field name="output_Q00" string="输出 Q2.1"/>
                                        <field name="output_Q00" string="输出 Q2.2"/>
                                        <field name="output_Q00" string="输出 Q2.3"/>
                                        <field name="output_Q00" string="输出 Q2.4"/>
                                        <field name="output_Q00" string="输出 Q2.5"/>
                                        <field name="output_Q00" string="输出 Q2.6"/>
                                        <field name="output_Q00" string="输出 Q2.7"/>
                                    </group>
                                    <group>
                                        <field name="output_Q00" string="输出 Q3.0"/>
                                        <field name="output_Q00" string="输出 Q3.1"/>
                                        <field name="output_Q00" string="输出 Q3.2"/>
                                        <field name="output_Q00" string="输出 Q3.3"/>
                                        <field name="output_Q00" string="输出 Q3.4"/>
                                        <field name="output_Q00" string="输出 Q3.5"/>
                                        <field name="output_Q00" string="输出 Q3.6"/>
                                        <field name="output_Q00" string="输出 Q3.7"/>
                                    </group>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>

        </field>
    </record>



    <record id="warehouse_control_system_act_window" model="ir.actions.act_window">
        <field name="name">自控系统</field>
        <field name="res_model">control.system</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>
