<odoo>

    <record id="warehouse_settings_view_list" model="ir.ui.view">
        <field name="name">warehouse_settings_view_list</field>
        <field name="model">warehouse.settings</field>
        <field name="arch" type="xml">
            <list>
                <field name="number_layers"/>
                <field name="number_column"/>
                <field name="lift_coordinate"/>
                <field name="shuttle_coordinate"/>
                <field name="layer_spacing"/>
                <field name="column_spacing"/>

            </list>
        </field>
    </record>

    <record id="warehouse_settings_act_window" model="ir.actions.act_window">
        <field name="name">基本参数</field>
        <field name="res_model">warehouse.settings</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>
